{
    "manifest_version": 2,
    "name": "Chrome Set Img Exten...",
    "version": "0.1",
    "web_accessible_resources": [
        "imgs/*.jpg"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["img.js"]
        }
    ]
}