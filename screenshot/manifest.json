{
    "manifest_version": 2,
    "name": "ScreenShot Extension",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "imgs/extension.png",
        "default_popup": "./sketch/index.html",
        "default_title": "Screenshot"
    }
}