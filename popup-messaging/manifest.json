{
    "manifest_version": 2,
    "name": "Popup Messaging Extension",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "imgs/extension.png",
        "default_popup": "./sketch/index.html",
        "default_title": "A popup will come here."
    }
}